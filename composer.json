{
    "name": "omelya/yii2-phpmailer",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "Адаптер PHPMailer для Yii2",
    "keywords": ["omelya", "yii2", "phpmailer", "mail", "adaper"],
    "authors": [
        {
            "name": "Vlad Omelianenko",
            "email": "sekret.krut@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "dicr\\phpmailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dicr\\tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0 || ^8.1",

        "yiisoft/yii2": "^2.0",

        "phpmailer/phpmailer": "^6.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^10.3"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
